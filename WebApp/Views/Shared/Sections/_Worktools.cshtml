@model WorktoolsSection

<section class="work-tools">
    <div class="container">
        <div class="section-title">
            <h1>@Model.Title</h1>
            <p>@Model.Description</p>
        </div>

        <div class="tool-boxes">

            @foreach(var toolBox in Model.ToolBoxes)
            {
                <partial name="~/Views/Shared/Components/_ToolBox.cshtml" model="toolBox" />
            }
          
        </div>
    </div>
</section>